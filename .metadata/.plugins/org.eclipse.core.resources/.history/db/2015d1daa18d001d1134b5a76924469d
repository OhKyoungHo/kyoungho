$(function() {
	$(".icon.heart").click(function() {
		var src = $(this).find("img");//버튼에 올린다는 행동
		$(this).attr("src", src.replace("https://cdn-icons-png.flaticon.com/512/803/803087.png","https://cdn-icons-png.flaticon.com/512/812/812327.png"));//행동에 따른 리액션
	}, function() {
		var src = $(this).find("img");
		$(this).attr("src", src.replace("https://cdn-icons-png.flaticon.com/512/812/812327.png","https://cdn-icons-png.flaticon.com/512/803/803087.png"));
	});
});